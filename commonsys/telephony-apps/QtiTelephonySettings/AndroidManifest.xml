<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qti.telephonysettings"
    android:sharedUserId="android.uid.qtiphone">

    <uses-sdk android:minSdkVersion="31"
        android:targetSdkVersion="35"/>

    <uses-permission android:name="android.permission.MANAGE_USERS" />
    <uses-permission android:name="android.permission.INTERACT_ACROSS_PROFILES" />

    <overlay
        android:targetPackage="com.qti.telephonysettings"
        android:requiredSystemPropertyName="ro.boot.vendor.qspa.modem"
        android:requiredSystemPropertyValue="enabled"/>

    <application android:process="com.qti.phone"
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher_settings"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/QtiTelephonySettingsTheme">
        <activity
            android:name=".QtiTelephonySettingsActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-library android:name="com.qti.extphone.extphonelib"/>

    </application>

</manifest>

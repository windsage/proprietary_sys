<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.qualcomm.qti.ims"
      android:versionCode="1"
      android:versionName="1.0">

    <uses-sdk android:minSdkVersion="23"
        android:targetSdkVersion="35"/>

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_PRIVILEGED_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.qti.permission.BIND_QTI_IMS_SERVICE" />
    <uses-permission android:name="android.permission.WRITE_SECURE_SETTINGS" />
    <uses-permission android:name="android.permission.MODIFY_PHONE_STATE" />
    <uses-permission android:name="com.qti.permission.RECEIVE_ESSENTIAL_RECORDS_LOADED"/>

    <overlay
        android:targetPackage="com.qualcomm.qti.ims"
        android:requiredSystemPropertyName="ro.boot.vendor.qspa.modem"
        android:requiredSystemPropertyValue="enabled"/>

    <application>
        <activity android:name="ImsSettings"
            android:theme="@style/QtiImsSettingsTheme"
            android:configChanges="orientation|screenSize|keyboardHidden"
            android:uiOptions="splitActionBarWhenNarrow"
            android:exported="true">
         <intent-filter>
             <action android:name="org.codeaurora.IMS_SETTINGS" />
             <category android:name="android.intent.category.DEFAULT" />
         </intent-filter>
        </activity>
        <uses-library android:name="ims-ext-common"/>
        <uses-library android:name="qti-telephony-utils" />
    </application>
</manifest>
